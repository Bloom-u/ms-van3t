#     conf.check_nonfatal(header_name='stdint.h', define_name='HAVE_STDINT_H')

def configure(conf):
    conf.env.append_value("CPPFLAGS", "-fPIC")
    conf.env.append_value('INCLUDES', ['/usr/local/include'])
    conf.env.append_value('LIBPATH', ["/usr/local/lib"])

